<h3>Register</h3>

<form action=/user method='post'>
    <div>
        <label>
            Username<input type="text" name="username"/>
        </label>
        <label>
            Email<input type="email"/>
        </label>
        <label>
            Password<input type="password"/>
        </label>
        <button type="submit">Submit</button>
    </div>
</form>  


